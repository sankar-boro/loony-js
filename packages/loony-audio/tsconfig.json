{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "lib": ["ESNext", "DOM"],
      "typeRoots": ["../../node_modules/@types"],
      "target": "ESNext",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": ".",
      "outDir": "../../build",
      "allowSyntheticDefaultImports": true
    }
  }